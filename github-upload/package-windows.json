{
  "name": "solovideo-club-windows",
  "version": "1.0.0",
  "description": "SoloVideoClub - AplicaciÃ³n de streaming para servidores Jellyfin y Emby",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "tsx server/index.ts",
    "build": "npm run build:server && npm run build:client",
    "build:server": "esbuild server/index.ts --bundle --platform=node --outfile=dist/server.js --external:pg-native --external:sqlite3",
    "build:client": "vite build",
    "db:push": "drizzle-kit push",
    "install-windows": "npm install --production",
    "setup": "npm run install-windows && npm run build && npm run db:push"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.0",
    "axios": "^1.6.0",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.0",
    "ws": "^8.14.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@vitejs/plugin-react": "^4.0.0",
    "drizzle-kit": "^0.20.0",
    "esbuild": "^0.19.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}